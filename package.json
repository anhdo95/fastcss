{
  "name": "fastcss",
  "version": "1.0.0",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "preserve": "yarn build && cp dist/fast.css public",
    "serve": "serve -p 8080 public",
    "prebuild": "rm -rf dist",
    "build": "yarn swcify && node -r @swc/register scripts/build.js",
    "build:debug": "yarn swcify -s && node -r @swc/register scripts/build.js",
    "preswcify": "rm -rf lib",
    "swcify": "swc src -d lib",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.215",
    "@swc/register": "^0.1.10",
    "babel-jest": "^28.1.3",
    "color": "^4.2.3",
    "jest": "^28.1.2",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "postcss": "^8.4.14",
    "postcss-functions": "^4.0.2",
    "postcss-js": "^4.0.0",
    "postcss-preset-env": "^7.7.2",
    "postcss-selector-parser": "^6.0.10",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2"
  }
}
